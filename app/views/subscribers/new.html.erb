<div class="container">
       
        <div class="row">
            <div class="col-lg-12">




<h1 class="page-header">New Subscriber</h1>

<%= simple_form_for(@subscriber) do |f| %>
  <%= f.input :name %>

  <%= f.input :subscription_type, :collection => Subscriber::SUBSCRIPTION_TYPE, :label => "Subscription Type", :include_blank => false %>
  <%= f.input :phone_number %>
  <%= f.input :fax_number, :input_html => { :value => '--' } %>
  <%= f.input :first_address %>
  <%= f.input :second_address, :input_html => { :value => '--' } %>
  <%= f.input :email_address %>
  <%= f.input :zip_code, :input_html => { :value => '--' } %>

  </br>
  </br>

<center>
    <%= f.submit "Save Subscriber", data: { confirm: "Confirm Subscriber?" }, :class => 'loginBtn btn btn-default', :id => 'loginbtn' %>
    </br>
<% end %>

  </br>

<%= button_to "Cancel", subscribers_path, method: :get, data: { confirm: "Are you sure?" }, :class => 'loginBtn btn btn-default', :id => 'loginbtn' %>
</center>

			</div>
		</div>
	</div>

  </br></br>