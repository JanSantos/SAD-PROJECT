<div class="container">
       
        <div class="row">
            <div class="col-lg-12">



<h1 class="page-header">Edit Subscriber</h1>

<%= simple_form_for(@subscriber) do |f| %>
  <%= f.input :name %>
  Subscription Type<%= f.collection_select :subscription_type, Subscriber::SUBSCRIPTION_TYPE, :to_s, :to_s, 
       :include_blank => false %>
  <%= f.input :contact_person %>
  <%= f.input :phone_details %>
  <%= f.input :fax_details %>
  <%= f.input :first_address %>
  <%= f.input :second_address %>
  <%= f.input :email_address %>
  <%= f.input :zip_code %>
  <%= f.input :ip_address %>

  
  <%= f.submit "Save Changes" %>
<% end %>

<%= button_to "Cancel", subscribers_path, method: :get, data: { confirm: "Are you sure?" } %>

      </div>
    </div>
  </div>