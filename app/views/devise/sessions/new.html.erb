<div class="container">
        <div class="row">
            <div class="col-lg-12">
                <div class="indexj jumbotron">
  <h1>Welcome!</h1>
  <p>Welcome to LS Journals' Subscription System. <br>Please <b>log-in</b> below to access and modify the journals and subscribers databases.</p>
</div>




<div class="row">
 <div class="col-md-6 col-md-offset-3">

  


<%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
  <div class="form-group">
    <%= f.label :email %><br />
    <%= f.email_field :email, autofocus: true, :class => 'form-control', :id => 'exampleInputEmail1', :placeholder => 'Enter Email' %>
  </div>

  <div class="form-group">
    <%= f.label :password %><br />
    <%= f.password_field :password, autocomplete: "off", :class => 'form-control', :id => 'exampleInputPassword1', :placeholder => 'Enter Password' %>
  </div>

  <% if devise_mapping.rememberable? -%>
    <div class="form-group">
      <%= f.check_box :remember_me %>
      <%= f.label :remember_me %>
    </div>
  <% end -%>

   <div class="alert alert-info" role="alert"><span class="glyphicon glyphicon-alert 

" aria-hidden="true"></span> Usernames and passwords are case-sensitive.</div>

  <div class="actions"><center>
    <%= f.submit "Log in", :class => 'loginBtn btn btn-default', :id => 'loginbtn' %>
  </div></center>
<% end %>

</br>
</br>

</br>


</div>
</div>



<%= render "devise/shared/links" %>
